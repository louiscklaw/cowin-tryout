
(rules PCB cowin-tryout
  (snap_angle 
    fortyfive_degree
  )
  (autoroute_settings
    (fanout off)
    (eu.mihosoft.freerouting.autoroute on)
    (postroute on)
    (vias on)
    (via_costs 50)
    (plane_via_costs 5)
    (start_ripup_costs 100)
    (start_pass_no 4091)
    (layer_rule F.Cu
      (active on)
      (preferred_direction horizontal)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 2.2)
    )
    (layer_rule B.Cu
      (active on)
      (preferred_direction vertical)
      (preferred_direction_trace_costs 1.0)
      (against_preferred_direction_trace_costs 1.8)
    )
  )
  (rule
    (width 250.0)
    (clear 200.2)
    (clear 125.0 (type smd_to_turn_gap))
    (clear 50.0 (type smd_smd))
  )
  (padstack "Via[0-1]_800:400_um"
    (shape
      (circle F.Cu 800.0 0.0 0.0)
    )
    (shape
      (circle B.Cu 800.0 0.0 0.0)
    )
    (attach off)
  )
  (via 
    "Via[0-1]_800:400_um" "Via[0-1]_800:400_um" default
  )
  (via 
    "Via[0-1]_800:400_um-kicad_default" "Via[0-1]_800:400_um" "kicad_default"
  )
  (via_rule
    default "Via[0-1]_800:400_um"
  )
  (via_rule
    "kicad_default" "Via[0-1]_800:400_um-kicad_default"
  )
  (class default
    (clearance_class default)
    (via_rule default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
  (class "kicad_default"
    GND +3V3 "Net-(C3-Pad1)" +5V "/esp-12e/nRST" "Net-(C14-Pad1)" "Net-(C15-Pad1)" "Net-(C16-Pad1)"
    "Net-(D1-Pad2)" "Net-(D2-Pad2)" "Net-(D3-Pad2)" "Net-(D3-Pad4)" "Net-(D4-Pad2)" "Net-(D7-Pad2)" "/esp-12e/GPIO16" /SDA
    /SCL "/pm2_5/SEN_RESET" /TXD /RXD /GPIO12 /SHT3X/NRST /SHT3X/ALERT "/esp-12e/GPIO2"
    "Net-(JP1-Pad2)" /SHT3X/ADDR "Net-(Q1-Pad1)" "/esp-12e/usb_to_uart/RTS" "Net-(Q2-Pad1)" "/esp-12e/GPIO0" "/esp-12e/usb_to_uart/DTR" "/sunlight_uv/EN"
    "/esp-12e/EN" "/esp-12e/GPIO15" "/esp-12e/usb_to_uart/RXD0" "Net-(R14-Pad2)" "Net-(R15-Pad2)" "/esp-12e/ADC" "Net-(U7-Pad7)" "Net-(U7-Pad6)"
    "Net-(C17-Pad1)" "Net-(C18-Pad1)" "Net-(D8-Pad2)" Vbat "Net-(D9-Pad1)" "Net-(D9-Pad2)" VBUS "Net-(JP6-Pad1)"
    "Net-(R11-Pad1)" "Net-(R20-Pad2)"
    (clearance_class "kicad_default")
    (via_rule kicad_default)
    (rule
      (width 250.0)
    )
    (circuit 
      (use_layer F.Cu B.Cu)
    )
  )
)